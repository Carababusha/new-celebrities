@if(Gender.HasValue && Gender == NewCelebrities.Shared.Gender.Male)
{
    <MaleIcon Style="@Style" />
}

@if(Gender.HasValue && Gender == NewCelebrities.Shared.Gender.Female)
{
    <FemaleIcon Style="@Style" />
}

@code {

    [Parameter]
    public NewCelebrities.Shared.Gender? Gender { get; set; }

    [Parameter]
    public string Style { get; set; } = Styles.Icon;
}
